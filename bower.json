  "kewords": [
    "AngularJS",
    "Material Design",
    "Framework"
  ],
  "homepage": "http://ui.lumapps.com",
  "version": "0.0.0",
  "authors": [
    "LumApps",
    "shilon5"
  ],
  "license": "MIT",
  "main": [
