{
    "name": "lumx",
    "description": "The first responsive front-end framework based on AngularJS & Google Material Design specifications",
    "kewords": [
        "AngularJS",
        "Material Design",
        "Framework"
    ],
    "homepage": "http://ui.lumapps.com",
    "version": "0.0.0",
    "authors": [
        "LumApps"
    ],
    "license": "MIT",
    "main": [
        "dist/css/lumx.css",
        "dist/lumx.js",
        "dist/fonts/*"
    ],
    "repository":
    {
        "type": "git",
        "url": "git://github.com/lumapps/lumx.git"
    },
    "ignore": [
        "**/.*",
        "node_modules",
        "libs",
        "demo",
        "gulpfile.js",
        "build",
        "*.md",
        "package.json",
        "index.html",
        "app.yaml",
        "app.js",
        "launch.sh",
        "/scss",
        "/favicon.png",
        "/fonts",
        "/js",
        "release.py"
    ],
    "dependencies":
    {
        "angular": "latest",
        "jquery": "latest",
        "moment": "latest",
        "bourbon": "latest"
    },
    "devDependencies":
    {
        "angular-ui-router": "~0.2.15",
        "angular-highlightjs": "~0.5.1"
    }
}